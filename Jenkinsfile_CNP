#!groovy

@Library("Infrastructure@DTSPO-16115-deprecate-properties-volume-spring-boot-starter") _

def type = "java"
def product = "toffee"
def component = "recipe-backend"

static LinkedHashMap<String, Object> secret(String secretName, String envVar) {
  [ $class: 'AzureKeyVaultSecret',
    secretType: 'Secret',
    name: secretName,
    version: '',
    envVariable: envVar
  ]
}

withPipeline(type, product, component) {}
